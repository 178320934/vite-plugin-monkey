{
  "name": "vite-plugin-monkey",
  "version": "0.2.13",
  "description": "vite plugin for Tampermonkey, Violentmonkey and Greasemonkey",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "pnpm-lock.yaml",
    "tsup.config.ts",
    "tsconfig.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "monkey",
    "Tampermonkey",
    "Violentmonkey",
    "Greasemonkey"
  ],
  "author": "lisonge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisonge/vite-plugin-monkey.git"
  },
  "bugs": {
    "url": "https://github.com/lisonge/vite-plugin-monkey/issues"
  },
  "homepage": "https://github.com/lisonge/vite-plugin-monkey#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "14.18.12",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vite": "2.9.6"
  },
  "peerDependencies": {
    "@types/greasemonkey": "^4.0.2",
    "@types/tampermonkey": "^4.0.5",
    "vite": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@types/greasemonkey": {
      "optional": true
    },
    "@types/tampermonkey": {
      "optional": true
    }
  },
  "engines": {
    "pnpm": "^6.0.0",
    "node": "^14.13.1 || >= 16"
  },
  "dependencies": {
    "cross-spawn": "7.0.3",
    "open": "8.4.0",
    "picocolors": "1.0.0"
  }
}
