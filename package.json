{
  "name": "vite-plugin-monkey",
  "version": "1.0.0-alpha.3",
  "description": "vite plugin for Tampermonkey, Violentmonkey and Greasemonkey",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "pnpm-lock.yaml",
    "tsup.config.ts",
    "tsconfig.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "monkey",
    "Tampermonkey",
    "Violentmonkey",
    "Greasemonkey"
  ],
  "author": "lisonge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisonge/vite-plugin-monkey.git"
  },
  "bugs": {
    "url": "https://github.com/lisonge/vite-plugin-monkey/issues"
  },
  "homepage": "https://github.com/lisonge/vite-plugin-monkey#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/detect-port": "1.3.2",
    "@types/node": "14.18.12",
    "@types/node-fetch": "2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "3.0.0"
  },
  "peerDependencies": {
    "@types/greasemonkey": "^4.0.2",
    "@types/tampermonkey": "^4.0.5",
    "vite": "^2.0.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "@types/greasemonkey": {
      "optional": true
    },
    "@types/tampermonkey": {
      "optional": true
    }
  },
  "engines": {
    "pnpm": ">=6",
    "node": "^14.13.1 || >= 16"
  },
  "dependencies": {
    "cross-spawn": "7.0.3",
    "detect-port": "1.3.0",
    "htmlparser2": "8.0.1",
    "node-fetch": "2.6.7",
    "open": "8.4.0",
    "picocolors": "1.0.0"
  },
  "volta": {
    "node": "16.15.1"
  }
}
